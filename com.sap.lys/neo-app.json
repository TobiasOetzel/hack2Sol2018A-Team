      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    }
  ],
  "headerWhiteList": [
    "sap-iot-thingtype",
    "sap-iot-eventtype",
    "sap-iot-pst"
  ]
