{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.sap.lys",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"EventService": {
				"uri": "/IOTAS-COMPOSITE-EVENTS-ODATA/CompositeEvents/EventType/v1/com.sap.appiot.eventtypes:StandardEventType",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/EventsMetadata/metadata.xml"
				}
			},
			"mainService": {
				"uri": "/IOTAS-ADVANCEDLIST-THING-ODATA/CompositeThings/v1/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"Thing_Service1": {
				"uri": "/IOTAS-DETAILS-THING-ODATA/CompositeThings/ThingType/v1/hack2sol.team0.hack2sol.ateam:bulb",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/Thing_Metadata1/metadata.xml"
				}
			}
		},
		"title": "{{appTitle}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "ui.appbuilder.iotappmtatemplatelayout",
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.sap.lys.view.Launcher",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.lys.i18n.i18n"
				}
			},
			"events": {
				"dataSource": "EventService",
				"preload": true,
				"settings": {
					"useBatch": false,
					"defaultCountMode": "Inline"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"useBatch": false,
					"defaultCountMode": "Inline",
					"headers": {
						"sap-iot-eventtype": "com.sap.appiot.eventtypes:StandardEventType",
						"sap-iot-pst": "hack2sol.team0.hack2sol.ateam:Image,hack2sol.team0.hack2sol.ateam:bulbMetaData,hack2sol.team0.hack2sol.ateam:bulbState"
					}
				}
			},
			"thingModel1": {
				"dataSource": "Thing_Service1",
				"preload": true,
				"settings": {
					"useBatch": false,
					"defaultCountMode": "Inline"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sap.lys.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
			},
			"routes": [
				{
					"pattern": "Detail/{ThingId}",
					"name": "Detail_binded",
					"target": [
						"Detail"
					]
				},
				{
					"pattern": "Main",
					"name": "Main",
					"target": "Main"
				},
				{
					"pattern": "",
					"name": "Launcher",
					"target": "Launcher"
				}
			],
			"targets": {
				"Main": {
					"viewType": "XML",
					"viewName": "Main",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "com.sap.lys.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"Detail": {
					"viewType": "XML",
					"viewName": "Detail",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "com.sap.lys.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"Launcher": {
					"viewType": "XML",
					"viewName": "Launcher"
				}
			},
			"additionalData": {
				"com.sap.lys.view.Detail": {
					"routeName": "Detail_binded",
					"entitySet": "/Things"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}